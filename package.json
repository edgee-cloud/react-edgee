{
  "name": "react-edgee",
  "version": "1.2.2",
  "description": "React component to use the Edgee SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build-lib": "tsup",
    "lint": "eslint --fix --cache src/",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edgee-cloud/react-edgee.git"
  },
  "keywords": [
    "Next",
    "Next Edgee",
    "Next.js",
    "Next.js 14",
    "Next.js 13",
    "React",
    "Edgee SDK",
    "Edgee"
  ],
  "author": "Sacha Morard",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/edgee-cloud/react-edgee/issues"
  },
  "homepage": "https://github.com/edgee-cloud/react-edgee#readme",
  "files": [
    "dist/**/*.js*",
    "dist/**/*.mjs*",
    "dist/**/*.d*"
  ],
  "devDependencies": {
    "@swc/core": "^1.6.13",
    "@types/node": "^18.19.40",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "esbuild": "^0.25.0",
    "eslint": "^8.35.0",
    "next": "^14.2.20",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rollup": "^4.22.4",
    "tsup": "^8.1.1",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "next": ">= 6.0.0",
    "react": ">= 16.0.0",
    "react-dom": ">= 16.0.0"
  },
  "dependencies": {
    "prop-types": "^15.8.1"
  }
}
